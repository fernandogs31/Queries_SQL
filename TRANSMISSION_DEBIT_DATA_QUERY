SELECT IVDD.DECLARACAO_ID, IVDD.NR_TRANSMISSAO, SUM(IVDD.VALOR_MN), IVDD.CREATION_DATE, IVD.NR_DECLARACAO, IVD.DT_DECLARACAO,IVE.ORGANIZACAO, IVE.EMBARQUE_NUM, IVE.EMBARQUE_ANO
FROM IMP_VW_DCL_DARF IVDD, IMP_VW_DECLARACOES_BRW IVD, IMP_VW_EMBARQUES IVE
WHERE IVD.DT_DECLARACAO BETWEEN '01/03/21' AND '31/03/21'
AND IVD.DECLARACAO_ID = IVDD.DECLARACAO_ID
AND IVD.EMBARQUE_ID = IVE.EMBARQUE_ID
GROUP BY IVDD.DECLARACAO_ID, IVDD.NR_TRANSMISSAO, IVDD.CREATION_DATE, IVD.NR_DECLARACAO, IVD.DT_DECLARACAO,IVE.ORGANIZACAO,IVE.EMBARQUE_NUM, IVE.EMBARQUE_ANO
ORDER BY 6 ASC;

SELECT IVDD.DECLARACAO_ID,IVDD.NR_TRANSMISSAO, IVD.NR_DECLARACAO, IVD.DT_DECLARACAO,IVE.ORGANIZACAO, IVE.EMBARQUE_NUM, IVE.EMBARQUE_ANO, IVNLE.NR_REFERENCIA, IVDD.CD_RECEITA_ID, CMB.CODIGO,CMB.DESCRICAO, IVDD.VALOR_MN, IVDD.creation_date
FROM IMP_VW_DCL_DARF IVDD,IMP_VW_DECLARACOES_BRW IVD, IMP_VW_EMBARQUES IVE, CMX_TABELAS_B CMB, IMP_VW_NUMERARIOS_LINHA_EBQ IVNLE
WHERE IVD.DT_DECLARACAO BETWEEN '01/03/21' AND '31/03/21'
--AND IVDD.CREATION_DATE > '01/03/21'
AND IVD.DECLARACAO_ID = IVDD.DECLARACAO_ID
AND IVD.EMBARQUE_ID = IVE.EMBARQUE_ID
AND IVDD.CD_RECEITA_ID = CMB.TABELA_ID
AND IVNLE.EMBARQUE_ID = IVE.EMBARQUE_ID
--AND IVDD.CD_RECEITA_ID =
