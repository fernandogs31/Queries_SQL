SELECT GJL.JE_HEADER_ID,
       GJL.LEDGER_ID,
       GJL.PERIOD_NAME,
       GJL.CREATION_DATE,
       GJL.DESCRIPTION,
       GJL.ENTERED_DR,
       GJL.ENTERED_CR,
       GJL.ACCOUNTED_DR,
       GJL.ACCOUNTED_CR,
       GL.NAME
    
FROM GL_JE_LINES GJL,
     GL_LEDGERS GL
     
WHERE GL.LEDGER_ID = GJL.LEDGER_ID
AND GL.NAME LIKE '%GEMSAM_BRA%'
AND GJL.CREATION_DATE BETWEEN '01/05/2020' AND '21/08/2020'
AND GJL.DESCRIPTION LIKE '%TAXA%';
