SELECT IVCE.DECLARACAO_ID,
       IDD.VALOR_MN,
       IDD.CD_RECEITA_ID,
       IVCE.EMBARQUE_NUM,
       IVCE.EMBARQUE_ANO,
       INE.CHAVE_DE_ACESSO,
       INE.NUMERO_DOCUMENTO_FISCAL,
       INE.DATA_EMISSAO_DOCUMENTO_FISCAL,
       INE.SERIE

FROM IMP_VW_CONSULTA_EBQ IVCE,
     IMP_DCL_DARF IDD,
     IMP_NF_ELETRONICA INE
     
WHERE IVCE.ORGANIZACAO_CODE = 'U41'
AND IVCE.EMBARQUE_NUM IN ('3128')
AND IVCE.EMBARQUE_ANO = '2020'
AND IVCE.DECLARACAO_ID = IDD.DECLARACAO_ID
AND IVCE.DECLARACAO_ID = INE.DECLARACAO_ID;
