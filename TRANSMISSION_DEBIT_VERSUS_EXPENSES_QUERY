SELECT DISTINCT
IDD.NR_DECLARACAO,
IVDAG.EMBARQUE,
IVDAG.NUMERARIO,
IVDD.NR_TRANSMISSAO,
IVDD.VALOR_MN,
IVDD.CREATION_DATE,
IVDAG.PO_CONTA,
IVDAG.GL_CONTA

FROM IMP_VW_DCL_DARF IVDD,
IMP_DECLARACOES IDD,
IMP_VW_DESP_AP_GL_GEHC IVDAG

WHERE IVDD.CREATION_DATE BETWEEN '01/03/2021' AND '23/03/2021'
AND IDD.DECLARACAO_ID = IVDD.DECLARACAO_ID
AND IDD.NR_DECLARACAO = IVDAG.NR_DECLARACAO
AND IVDAG.DESPESA = 'TX_SISCOMEX_RETIF_GL';

/
SELECT DISTINCT
IDD.NR_DECLARACAO,
IE.EMBARQUE_NUM,
IE.EMBARQUE_ANO,
II.NR_REFERENCIA,
IVDD.NR_TRANSMISSAO,
IVDD.VALOR_MN,
IVDD.CREATION_DATE
--IVDAG.PO_CONTA,
--IVDAG.GL_CONTA

FROM ecomex.IMP_DCL_DARF IVDD,
ecomex.IMP_DECLARACOES IDD,
ecomex.IMP_NUMERARIOS II,
ecomex.IMP_EMBARQUES IE
--ecomex.IMP_VW_DESP_AP_GL_GEHC IVDAG

WHERE IVDD.CREATION_DATE BETWEEN '01/10/2021' AND '26/11/2021'
AND IDD.DECLARACAO_ID = IVDD.DECLARACAO_ID
AND IVDD.NUMERARIO_ID = II.NUMERARIO_ID
AND IDD.EMBARQUE_ID = IE.EMBARQUE_ID
--AND IDD.NR_DECLARACAO = IVDAG.NR_DECLARACAO
--AND IVDAG.DESPESA = 'TX_SISCOMEX_RETIF_GL'
