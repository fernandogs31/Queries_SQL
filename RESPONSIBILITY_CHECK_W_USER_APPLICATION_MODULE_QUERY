SELECT 
  FRVL.RESPONSIBILITY_ID AS "ID RESPONSABILIDADE",
  FRVL.LAST_UPDATE_DATE AS "DATA ATUALIZAÇÃO",
  FRVL.CREATION_DATE AS "DATA CRIAÇÃO",
  FRVL.RESPONSIBILITY_NAME AS "RESPONSABILIDADE",
  FURGA.USER_ID AS "ID USUARIO",
  FURGA.START_DATE AS "DATA INICIO RESPONSABILIDADE",
  FURGA.END_DATE AS "DATA TERMINO RESPONSABILIDADE",
  AU.USERNAME AS "NOME USUARIO",
  FA.APPLICATION_ID AS "ID APLICAÇÃO",
  FATL.APPLICATION_NAME AS "NOME APLICAÇÃO",
  FA.APPLICATION_SHORT_NAME AS "NOME ABREVIADO APLICAÇÃO",
  FA.BASEPATH AS "CAMINHO"

FROM 
  FND_RESPONSIBILITY_VL FRVL,
  FND_USER_RESP_GROUPS_ALL FURGA,
  ALL_USERS AU,
  FND_APPLICATION FA,
  FND_APPLICATION_TL FATL

WHERE 
     FRVL.RESPONSIBILITY_NAME LIKE 'GEHC%'
AND  FRVL.RESPONSIBILITY_ID = FURGA.RESPONSIBILITY_ID
AND  FRVL.APPLICATION_ID = FATL.APPLICATION_ID
AND  FATL.APPLICATION_ID = FA.APPLICATION_ID
AND  FURGA.USER_ID = AU.USER_ID;
