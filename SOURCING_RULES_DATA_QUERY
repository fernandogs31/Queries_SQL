SELECT MP.ORGANIZATION_CODE RECEIPT_ORG,
       MSI.SEGMENT1 ITEM, 
       MSR.SOURCING_RULE_NAME,  
       MAS.ASSIGNMENT_SET_NAME,  
       MP1.ORGANIZATION_CODE SOURCE_ORG,
       msso.ALLOCATION_PERCENT "%", 
       msso.RANK,
       MSR.PLANNING_ACTIVE

FROM  apps.MRP_SOURCING_RULES MSR,
      APPS.MRP_SR_RECEIPT_ORG MSO,
      APPS.MRP_SR_SOURCE_ORG_V MSSO,
      APPS.MTL_PARAMETERS MP,
      APPS.MRP_SR_ASSIGNMENTS MSA,
      apps.MRP_ASSIGNMENT_SETS MAS,
      APPS.MTL_SYSTEM_ITEMS MSI,
      APPS.MTL_PARAMETERS MP1

 WHERE MSO.SOURCING_RULE_ID = MSR.SOURCING_RULE_ID
     AND MSA.SOURCING_RULE_ID = MSR.SOURCING_RULE_ID
     AND MP.ORGANIZATION_ID (+)= MSO.RECEIPT_ORGANIZATION_ID
     AND MSO.SR_RECEIPT_ID = MSSO.SR_RECEIPT_ID
     AND MAS.ASSIGNMENT_SET_ID = MSA.ASSIGNMENT_SET_ID
     AND MSA.INVENTORY_ITEM_ID = MSI.INVENTORY_ITEM_ID
     AND MSA.ORGANIZATION_ID = MSI.ORGANIZATION_ID
     AND MSA.ORGANIZATION_ID = MP.ORGANIZATION_ID
     AND MP1.ORGANIZATION_ID (+)= MSSO.SOURCE_ORGANIZATION_ID
     AND MP.ORGANIZATION_CODE = 'C83'
  -- AND MSI.SEGMENT1 = 'M1210238';
