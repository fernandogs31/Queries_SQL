SELECT
II.INVOICE_NUM    "NR_INVOICE",
II.DT_EMISSAO     "DT_EMISSAO",
CO.CODIGO         "ORG",
IIL.INVOICE_LIN_ID "ID_LINHA",
IILD.INVOICE_LIN_DET_ID "ID_COMPOSTO_DE"

FROM 
IMP_INVOICES II,
CMX_ORGANIZACOES CO,
IMP_INVOICES_LIN IIL,
IMP_INVOICES_LIN_DET IILD

WHERE CO.ORGANIZACAO_ID = II.ORGANIZACAO_ID
AND CO.CODIGO = 'C83'
AND II.INVOICE_ID = IIL.INVOICE_ID
AND II.DT_EMISSAO BETWEEN '01-01-2018' AND '31-08-2020'
AND IIL.INVOICE_LIN_ID = IILD.INVOICE_LIN_ID;
