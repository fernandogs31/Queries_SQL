SELECT DISTINCT EMPRESA,
ORGANIZACAO,
EMBARQUE_NUM,
EMBARQUE_ANO,
NR_DECLARACAO,
DT_DECLARACAO,
DESPESA_PARA,
SUM(VALOR),
DESTINO,
NF_TP_NOTA,
NF_NUMERO,
NF_SERIE,
NF_DT_EMISSAO,
NF_ID_ENVIO_RECEBTO,
NF_DT_ENVIO_RECEBTO,
CONTA_PO,
CONTA_GL,
CONTA_AP

FROM IMP_VW_DESPESAS_NF_DCC_GEHC

WHERE DT_DECLARACAO BETWEEN '01/01/2020' AND '31/12/2020'
AND EMPRESA IN ('CONTAGEM','RECIFE','S√ÉO PAULO')
GROUP BY
EMPRESA,
ORGANIZACAO,
EMBARQUE_NUM,
EMBARQUE_ANO,
NR_DECLARACAO,
DT_DECLARACAO,
DESPESA_PARA,
VALOR,
DESTINO,
NF_TP_NOTA,
NF_NUMERO,
NF_SERIE,
NF_DT_EMISSAO,
NF_ID_ENVIO_RECEBTO,
NF_DT_ENVIO_RECEBTO,
CONTA_PO,
CONTA_GL,
CONTA_AP
