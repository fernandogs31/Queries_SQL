SELECT *
FROM IMP_VW_NUMERARIOS_EBQ
WHERE ORGANIZACAO_CODIGO IN ('ASP','DSP')
AND EMBARQUE_ANO = '2020'
AND EMBARQUE_NUM = '14'
/
SELECT *
FROM IMP_NOTA_FISCAL
WHERE DECLARACAO_ID = '169013'
/
SELECT EMBARQUE_ID, DECLARACAO_ID
FROM IMP_VW_CONSULTA_EBQ
WHERE NR_DECLARACAO = '2002284830'
/
SELECT *
FROM IMP_VW_DOCUMENTO_COMPL
WHERE CODIGO = 'S√ÉO PAULO'
AND EMBARQUE_ANO = '2020'
AND EMBARQUE_NUM = '14'
/
SELECT *
FROM CMX_VW_DOCUMENTOS_ANEXOS
WHERE DOCUMENTO_ID = '160370'
